# =============================================================================
# Docker Compose Environment Variables - EXAMPLE
# =============================================================================
# Copie este arquivo para .env e ajuste os valores para seu ambiente
# =============================================================================

# ---- Frontend Build Variables ------------------------------------------------
VITE_API_URL=https://api-back.example.com
VITE_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

VITE_APP_BASE_PATH=/
VITE_APP_NAME=LiveChat
VITE_APP_VERSION=1.0.0
VITE_PUBLIC_URL=https://app.example.com

# ---- Feature Flags -----------------------------------------------------------
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_PASSWORD_RESET=true
VITE_ENABLE_OAUTH=false
